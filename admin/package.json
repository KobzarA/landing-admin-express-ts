{
  "name": "admin-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "formik": "^2.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.3",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-https": "cross-env HTTPS=true react-scripts start",
    "build": "react-scripts build && pnpm cp_l",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cp": "cross-env OS_TYPE=%PROCESSOR_ARCHITECTURE% && (echo %OS_TYPE% | findstr /c:\"x86\" >nul && xcopy /s /e /y build\\* ..\\server\\public\\admin || cp -r build/* ../server/public/admin)",
    "cp_w": "xcopy /s /e /y /I build\\* ..\\server\\public\\admin",
    "cp_l": "mkdir -p ../server/public/admin && cp -r build/* ../server/public/admin"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.48",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "react-test-renderer": "^18.2.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "proxy": "https://localhost:8000"
}
